{
    "env": {
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": ["prettier", "airbnb-base"],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-console": 0,
        "indent": 0,
        "linebreak-style": 0
    }
}